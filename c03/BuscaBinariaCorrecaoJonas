#include <iostream>
#include <string>

using namespace std;

struct Aluno
{
	int matricula;
	string nome;
	float nota;
}

int busca_seq_aluno_matricula (Aluno alunos[], int matricula_bubscada, int tamanho)
{
	   for(int 1 = 0; i < tamanho; i++) // 1+3n+3n
	   {
	   	if(alunos[i].matricula == matricula_buscada) // 4n
	   		return i;
	   }
	   return -1;
} // 10n + 1

